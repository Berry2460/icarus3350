CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE TEXTURES(TEXTURE_NAME VARCHAR(25) NOT NULL PRIMARY KEY,FILE_PATH VARCHAR(50))
