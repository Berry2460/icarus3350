CREATE MEMORY TABLE AUDIO(AUDIO_NAME VARCHAR(25) NOT NULL PRIMARY KEY,FILE_PATH VARCHAR(50))
CREATE MEMORY TABLE SAVE(VOLUME FLOAT,DIFFICULTY INTEGER,CURRENT_LEVEL INTEGER,SEED INTEGER,SAVE_EXISTS INTEGER,GAME_MUTED INTEGER,MAX_LEVEL INTEGER,MUSIC_MUTED INTEGER)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
